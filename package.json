{
  "devDependencies": {
    "@types/lodash": "4.14.167",
    "@types/node": "12.12.31",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "ava": "2.4.0",
    "husky": "5.0.6",
    "lint-staged": "10.5.3",
    "prettier-standard": "16.4.1",
    "semantic-release-cli": "5.4.1",
    "standardx": "7.0.0",
    "typescript": "4.1.3",
    "semantic-release": "17.3.2"
  },
  "name": "primitivify",
  "description": "deep copy data keeping only primitives in plain array or object data structures",
  "version": "0.0.0-development",
  "main": "src/index.js",
  "repository": "git@github.com:cdaringe/primitivify.git",
  "author": "cdaringe <cdaringe@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "is-plain-object": "^4.0.0",
    "lodash": ">=4"
  },
  "files": ["src"],
  "scripts": {
    "build": "tsc",
    "test": "ava && node test/test.commonjs.import.js",
    "format": "prettier-standard '{src,test,scripts}/**/*.{js,jsx,ts,tsx}'",
    "lint": "standardx '{src,test,scripts}/**/*.{js,jsx,ts,tsx}' --fix",
    "semantic-release": "semantic-release"
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "test/**/*.test.js"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,scripts}/**/*.{js,jsx,ts,tsx}": [
      "yarn format",
      "yarn lint",
      "git add"
    ]
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": 0
    }
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "ignore": [
      "**/*.d.ts"
    ]
  },
  "keywords": [
    "clone",
    "copy",
    "deep",
    "primitive",
    "serialize",
    "serialization",
    "stringify",
    "json"
  ]
}
